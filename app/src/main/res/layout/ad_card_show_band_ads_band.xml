<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shadow_button"
    tools:context=".fragments.ShowBandAdsBandFragment">

    <TextView
        android:id="@+id/regionTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Regione"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/regionTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:maxLines="1"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="region"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regionTagTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/musicianTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Musicista Cercato"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regionTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/musicianTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:maxLines="1"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="musician"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/musicianTagTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/descriptionTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Descrizione"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/musicianTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/descriptionTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:inputType="textMultiLine|text"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="description"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTagTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/userTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Inserito Da"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/descriptionTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/userTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:inputType="textMultiLine|text"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="user"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/userTagTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/emailTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Email"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/emailTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:inputType="textMultiLine|text"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="email"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTagTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/phoneTagTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:shadowDx="1"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="Numero di Telefono"
        android:textAllCaps="true"
        android:textColor="@color/red_light"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextViewShowBandAdsBand" />

    <TextView
        android:id="@+id/phoneTextViewShowBandAdsBand"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="65dp"
        android:fontFamily="@font/boogaloo"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:shadowColor="@android:color/white"
        android:text="phone"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phoneTagTextViewShowBandAdsBand" />

</androidx.constraintlayout.widget.ConstraintLayout>